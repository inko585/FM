<ResourceDictionary x:Class="AEStyles" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:wpfBasis="clr-namespace:AE.Graphics.Wpf.Basis">

    <SolidColorBrush x:Key="MainBackGroundColor" Color="#FFE8EAEB" />
    <SolidColorBrush x:Key="MainFontColor" Color="#777" />
    <!--<SolidColorBrush x:Key="MainButtonColor" Color="#D32E0B" />-->
    <SolidColorBrush x:Key="MainButtonColor" Color="#FF3E94D3" />
    <SolidColorBrush x:Key="SecondaryButtonColor" Color="#FF383A3E"/>
    <SolidColorBrush x:Key="MainButtonFontColor" Color="White"/>
    <SolidColorBrush x:Key="SecondaryButtonFontColor" Color="White"/>
    <SolidColorBrush x:Key="SecondaryButtonHoverColor" Color="Orange"/>
    <SolidColorBrush x:Key="SecondaryButtonClickColor" Color="Orange"/>
    <SolidColorBrush x:Key="MainButtonHoverColor" Color="Orange"/>
    <SolidColorBrush x:Key="MainButtonClickColor" Color="Orange"/>
    <SolidColorBrush x:Key="NavAreaColor" Color="#2f4050"/>
    <SolidColorBrush x:Key="NavAreaSelectColor" Color="White"/>
    <SolidColorBrush x:Key="NavAreaHighlightColor" Color="White"/>
    <SolidColorBrush x:Key="NavAreaFontColor" Color="#8095a8"/>
    <SolidColorBrush x:Key="NavAreaBorderColor" Color="Black"/>
    <SolidColorBrush x:Key="ListColor" Color="White"/>
    <SolidColorBrush x:Key="ListSelectColor" Color="#FF3E94D3"/>
    <SolidColorBrush x:Key="ListHighlightColor" Color="#FF3E94D3"/>
    <SolidColorBrush x:Key="ListFontColor" Color="#333"/>
    <SolidColorBrush x:Key="ListBorderColor" Color="Black"/>
    <SolidColorBrush x:Key="ListHoverColor" Color="#FF50A3DF"/>
    <SolidColorBrush x:Key="WindowBorderColor" Color="#FF383A3E"/>
    <SolidColorBrush x:Key="SelectColor" Color="Green"/>
    <SolidColorBrush x:Key="FontHighlightColor" Color="#FF2399CC"/>
    <LinearGradientBrush x:Key="ListSelectColorGradient" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#D32E0B" Offset="1"/>
        <GradientStop Color="#FA2E0B" Offset="0"/>
    </LinearGradientBrush>


    <!-- AE Window -->
    <Style x:Key="AEWindowStyle" TargetType="Window">

        <Setter Property="WindowStyle" Value="None" />
        <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.WindowTextBrushKey}}"/>
        <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.WindowBrushKey}}"/>
        <Setter Property="Template" Value="{DynamicResource ResourceKey=AEWindowBorderTemplate}"/>

    </Style>

    <ControlTemplate x:Key="AEWindowBorderTemplate" TargetType="Window">

        <DockPanel x:Name="WindowDockPanel">

            <!-- Window Border -->
            <Grid x:Name="LeftBorderGrid" Background="{StaticResource WindowBorderColor}" DockPanel.Dock="Left" Width="3">
                <Grid.RowDefinitions>
                    <RowDefinition Height="20"/>
                    <RowDefinition />
                    <RowDefinition Height="20"/>
                </Grid.RowDefinitions>

                <Thumb Grid.Row="0" x:Name="LeftTopBorderThumb" Cursor="SizeNWSE" Style="{DynamicResource AEThumbStyle}"/>
                <Thumb Grid.Row="1" x:Name="LeftBorderThumb" Cursor="SizeWE" Style="{DynamicResource AEThumbStyle}"/>
                <Thumb Grid.Row="2" x:Name="LeftBottomBorderThumb" Cursor="SizeNESW" Style="{DynamicResource AEThumbStyle}"/>
            </Grid>

            <Grid x:Name="TopBorderGrid" Background="{StaticResource WindowBorderColor}" DockPanel.Dock="Top" Height="3" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="20"/>
                    <ColumnDefinition />
                    <ColumnDefinition Width="20"/>
                </Grid.ColumnDefinitions>

                <Thumb Grid.Column="0" x:Name="TopLeftBorderThumb" Cursor="SizeNWSE" Style="{DynamicResource AEThumbStyle}"/>
                <Thumb Grid.Column="1" x:Name="TopBorderThumb"  Cursor="SizeNS" Style="{DynamicResource AEThumbStyle}"/>
                <Thumb Grid.Column="2" x:Name="TopRightBorderThumb" Cursor="SizeNESW" Style="{DynamicResource AEThumbStyle}"/>
            </Grid>

            <Grid x:Name="RightBorderGrid" Background="{StaticResource WindowBorderColor}" DockPanel.Dock="Right" Width="3">
                <Grid.RowDefinitions>
                    <RowDefinition Height="20"/>
                    <RowDefinition />
                    <RowDefinition Height="20"/>
                </Grid.RowDefinitions>

                <Thumb Grid.Row="0" x:Name="RightTopBorderThum" Cursor="SizeNESW" Style="{DynamicResource AEThumbStyle}"/>
                <Thumb Grid.Row="1" x:Name="RightBorderThumb"  Cursor="SizeWE" Style="{DynamicResource AEThumbStyle}"/>
                <Thumb Grid.Row="2" x:Name="RightBottomBorderThumb" Cursor="SizeNWSE" Style="{DynamicResource AEThumbStyle}"/>
            </Grid>

            <Grid x:Name="BottomBorderGrid" Background="{StaticResource WindowBorderColor}" DockPanel.Dock="Bottom" Height="3">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="20"/>
                    <ColumnDefinition />
                    <ColumnDefinition Width="20"/>
                </Grid.ColumnDefinitions>

                <Thumb Grid.Column="0" x:Name="BottomLeftBorderThumb" Cursor="SizeNESW" Style="{DynamicResource AEThumbStyle}" />
                <Thumb Grid.Column="1" x:Name="BottomBorderThumb" Cursor="SizeNS" Style="{DynamicResource AEThumbStyle}"/>
                <Thumb Grid.Column="2" x:Name="BottomRightBorderThumb" Cursor="SizeNWSE" Style="{DynamicResource AEThumbStyle}" />

                <ResizeGrip x:Name="BottomBorderResizeGrip" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" Visibility="Collapsed"/>
            </Grid>
            <!-- Window Border end -->

            <!-- Window -->
            <Grid x:Name="WindowGrid">

                <Grid.Resources>
                    <ResourceDictionary>
                        <ResourceDictionary.MergedDictionaries>
                            <ResourceDictionary Source="pack://application:,,,/AE.Graphics.Wpf;component/Styles/AEStyles.xaml"/>
                            <ResourceDictionary>
                                <FontFamily x:Key="FontAwesome">/AE.Graphics.WPF;Component/Fonts/fontawesome-webfont.ttf#FontAwesome</FontFamily>
                            </ResourceDictionary>
                        </ResourceDictionary.MergedDictionaries>
                    </ResourceDictionary>
                </Grid.Resources>

                <Border x:Name="WindowBorder" Background="{StaticResource WindowBorderColor}" BorderBrush="{StaticResource WindowBorderColor}" >

                    <DockPanel x:Name="WindowContent" LastChildFill="True">

                        <Border x:Name="WindowBarBorder" Margin="0,0,0,0" Height="30" DockPanel.Dock="Top" BorderBrush="{StaticResource WindowBorderColor}" >

                            <DockPanel x:Name="WindowBar" Background="Transparent" LastChildFill="True">

                                <!-- Close Window Button -->
                                <Button
                                    x:Name="CloseWindowButton"
                                    Width="50"
                                    Height="35" 
                                    DockPanel.Dock="Right"
                                    Visibility="Hidden"
                                    VerticalAlignment="Center"
                                    Style="{DynamicResource AEWindowBorderCloseButtonStyle}">

                                    <Button.Content>
                                        <Grid Width="30" Height="25">
                                            <TextBlock x:Name="CloseWindowButtonIcon" Text="&#xf00d;" FontFamily="{StaticResource FontAwesome}" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="0,0,0,1" Foreground="White"/>
                                        </Grid>
                                    </Button.Content>
                                </Button>

                                <!-- Maximize Window Button -->
                                <Button
                                    x:Name="MaximizeWindowButton"
                                    Width="50"
                                    Height="35" 
                                    DockPanel.Dock="Right"  
                                    Visibility="Hidden"
                                    VerticalAlignment="Center" 
                                    HorizontalAlignment="Center" 
                                    Style="{DynamicResource AEWindowBorderButtonStyle}">

                                    <Button.Content>
                                        <Grid Width="31" Height="25">
                                            <TextBlock x:Name="MaximizeWindowButtonIcon" Text="&#xf2d0;" FontFamily="{StaticResource FontAwesome}" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="2,0,0,1" Foreground="White"/>
                                        </Grid>
                                    </Button.Content>
                                </Button>

                                <!-- Minimize Window Button -->
                                <Button
                                    x:Name="MinimizeWindowButton"
                                    Width="50"
                                    Height="35" 
                                    DockPanel.Dock="Right" 
                                    Visibility="Hidden"
                                    VerticalAlignment="Center" 
                                    HorizontalAlignment="Center" 
                                    VerticalContentAlignment="Bottom"
                                    Style="{DynamicResource AEWindowBorderButtonStyle}">

                                    <Button.Content>
                                        <Grid Width="30" Height="25">
                                            <TextBlock x:Name="MinimizeWindowButtonIcon" Text="&#xf2d1;" FontFamily="{StaticResource FontAwesome}" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center"  Padding="3.5,0,0,15"  Foreground="White"/>
                                        </Grid>
                                    </Button.Content>
                                </Button>

                                <!-- Nexans Logo -->
                                <!--<Image Source="pack://application:,,,/AE.Graphics.Wpf;component/nexans_cut.svg" Height="20" Width="20" Margin="8,0,4,2" VerticalAlignment="Center" />-->

                                <Viewbox x:Name="IconViewBox" Width="24" Height="18"  Margin="8,0,2,4" VerticalAlignment="Center">

                                    <ContentControl>
                                        <ContentPresenter x:Name="AppIcon" Content="{DynamicResource Nexans}">

                                        </ContentPresenter>

                                    </ContentControl>
                                </Viewbox>

                                <!-- Title -->
                                <TextBlock Margin="4,0,0,4" VerticalAlignment="Center" FontWeight="SemiBold" Foreground="White" TextWrapping="NoWrap" Text="{TemplateBinding Title}" FontSize="12" />

                            </DockPanel>

                        </Border>

                        <AdornerDecorator DockPanel.Dock="Bottom">
                            <ContentPresenter ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" ContentStringFormat="{TemplateBinding ContentStringFormat}"/>
                        </AdornerDecorator>

                    </DockPanel>

                </Border>

            </Grid>
            <!-- Window end -->

        </DockPanel>

    </ControlTemplate>

    <Style x:Key="AETabControlStyle"  TargetType="TabControl">
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Background" Value="{StaticResource ListColor}" />
    </Style>

    <Style x:Key="LinkLabelStyle"  TargetType="Label">
        <Style.Triggers>
            <Trigger  Property="IsMouseOver" Value="True">
                <Setter Property="Cursor" Value="Hand" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="AETabItemStyle" TargetType="TabItem">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TabItem">
                    <TextBlock Name="TabItemText" Margin="0,0,0,2">
                                                        <ContentPresenter x:Name="ContentSite" VerticalAlignment="Center" HorizontalAlignment="Center" ContentSource="Header" Margin="10,2"/>
                    </TextBlock>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="TabItemText" Property="Foreground" Value="{StaticResource FontHighlightColor}" />
                            <Setter TargetName="TabItemText" Property="TextDecorations" Value="Underline" />
                            <Setter TargetName="TabItemText" Property="FontWeight" Value="Bold" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="AETabItemRotateStyle" TargetType="TabItem">
        <Setter Property="BorderBrush" Value="Black"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TabItem">
                    <Border BorderThickness="1" BorderBrush="Black">
                    <TextBlock Name="TabItemText" Margin="0,0,0,2">
                                                        <ContentPresenter x:Name="ContentSite" VerticalAlignment="Center" HorizontalAlignment="Center" ContentSource="Header" Margin="10,2">
                                                        <!--<ContentPresenter.LayoutTransform>
                                                            <RotateTransform Angle="270" />
                                                        </ContentPresenter.LayoutTransform>-->    
                                                        </ContentPresenter>
                    </TextBlock>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="TabItemText" Property="Foreground" Value="{StaticResource FontHighlightColor}" />
                            <Setter TargetName="TabItemText" Property="FontWeight" Value="Bold" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <Style x:Key="HorizontalAEList" TargetType="ListBox">
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="HorizontalAEItemBox" TargetType="ListBoxItem">
        <Style.Resources>
            <wpfBasis:VisibilityConverter x:Key="vconv" />
        </Style.Resources>

        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Margin" Value="2.5, 0, 2.5, 0"/>
        <Setter Property="BorderBrush" Value="{StaticResource ListColor}"/>
        <Setter Property="Visibility" Value="{Binding Path=IsVisible, Converter={StaticResource vconv}}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ListBoxItem">

                    <Border Name="border" 
                            BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}" 
                            Background="{TemplateBinding Background}" >

                        <ContentPresenter VerticalAlignment="Center" Margin="5, 0, 5, 0" />

                    </Border>

                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="False" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.Setters>
                                <Setter TargetName="border" Property="Background" Value="{StaticResource ListHoverColor}"/>
                                <Setter Property="Foreground" Value="White"/>
                            </MultiTrigger.Setters>
                        </MultiTrigger>
                        <Trigger Property="IsSelected" Value="true">
                            <Setter TargetName="border" Property="Background" Value="{StaticResource ListSelectColor}"/>
                            <Setter Property="Foreground" Value="White"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="border" Property="Opacity" Value="0.4"/>
                        </Trigger>
                    </ControlTemplate.Triggers>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="AEWindowBorderButtonStyle" TargetType="Button">
        <Setter Property="FocusVisualStyle">
            <Setter.Value>
                <Style>
                    <Setter Property="Control.Template">
                        <Setter.Value>
                            <ControlTemplate>
                                <Rectangle Margin="2" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="Background" Value="{StaticResource WindowBorderColor}"/>
        <Setter Property="BorderBrush" Value="#FF707070"/>
        <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="1"/>

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">

                    <Border 
                        x:Name="border" 
                        BorderBrush="{TemplateBinding BorderBrush}" 
                        BorderThickness="{TemplateBinding BorderThickness}" 
                        Background="{TemplateBinding Background}" >

                        <ContentPresenter 
                            x:Name="contentPresenter" 
                            ContentTemplate="{TemplateBinding ContentTemplate}" 
                            Content="{TemplateBinding Content}" 
                            ContentStringFormat="{TemplateBinding ContentStringFormat}" 
                            Focusable="False" 
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                            Margin="{TemplateBinding Padding}" 
                            RecognizesAccessKey="True"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>

                    </Border>

                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDefaulted" Value="True">
                            <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation To="#FFADB2B5" Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                        <ColorAnimation To="#FF3C7FB1" Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>

                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                        <ColorAnimation Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="border" Value="#FFC4E5F6"/>
                            <Setter Property="BorderBrush" TargetName="border" Value="#FF2C628B"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Background" TargetName="border" Value="#FFF4F4F4"/>
                            <Setter Property="BorderBrush" TargetName="border" Value="#FFADB2B5"/>
                            <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="#FF838383"/>
                        </Trigger>
                    </ControlTemplate.Triggers>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="AEWindowBorderCloseButtonStyle" TargetType="Button">
        <Setter Property="FocusVisualStyle">
            <Setter.Value>
                <Style>
                    <Setter Property="Control.Template">
                        <Setter.Value>
                            <ControlTemplate>
                                <Rectangle Margin="2" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="Background" Value="{StaticResource WindowBorderColor}"/>
        <Setter Property="BorderBrush" Value="#FF707070"/>
        <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="1"/>

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">

                    <Border 
                        x:Name="border" 
                        BorderBrush="{TemplateBinding BorderBrush}" 
                        BorderThickness="{TemplateBinding BorderThickness}" 
                        Background="{TemplateBinding Background}" >

                        <ContentPresenter 
                            x:Name="contentPresenter" 
                            ContentTemplate="{TemplateBinding ContentTemplate}" 
                            Content="{TemplateBinding Content}" 
                            ContentStringFormat="{TemplateBinding ContentStringFormat}" 
                            Focusable="False" 
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                            Margin="{TemplateBinding Padding}" 
                            RecognizesAccessKey="True"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>

                    </Border>

                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDefaulted" Value="True">
                            <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation To="Red" Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                        <ColorAnimation To="#FF3C7FB1" Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>

                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetProperty="(Control.Background).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                        <ColorAnimation Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" FillBehavior="HoldEnd" Duration="0:0:0.25" AutoReverse="False" RepeatBehavior="1x" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="border" Value="#FFC4E5F6"/>
                            <Setter Property="BorderBrush" TargetName="border" Value="#FF2C628B"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Background" TargetName="border" Value="#FFF4F4F4"/>
                            <Setter Property="BorderBrush" TargetName="border" Value="#FFADB2B5"/>
                            <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="#FF838383"/>
                        </Trigger>
                    </ControlTemplate.Triggers>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type GroupItem}" x:Key="NavAreaContainerStyle">
        <Setter Property="Margin" Value="0,15,0,0"/>
        <Setter Property="Template">

            <Setter.Value>
                <ControlTemplate TargetType="GroupItem">

                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>



                        <Border BorderBrush="DarkGray" BorderThickness="0,0,0,1" Margin="5,0,5,8" >
                            <ContentControl Content="{TemplateBinding Content}"/>
                        </Border>


                        <Border Name="Content" Grid.Row="1">
                            <ItemsPresenter></ItemsPresenter>
                        </Border>


                    </Grid>

                </ControlTemplate>

            </Setter.Value>
        </Setter>
    </Style>


    <x:Code>

        <![CDATA[
                    void Expander_PreviewMouseDown(object sender, MouseButtonEventArgs e)
                       {
                         FrameworkElement fe = e.OriginalSource as FrameworkElement;
                            if (fe is TextBlock && fe.Name == "HeaderText")
                             {
                               var expander = sender as Expander;
                               expander.IsExpanded = !expander.IsExpanded;
                              }
                    }]]>

    </x:Code>

    <Style TargetType="{x:Type GroupItem}" x:Key="NavAreaExpandableContainerStyle" >
        <Setter Property="Margin" Value="0,15,0,0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Expander Header="Name" IsExpanded="True" >
                        <Expander.Style>
                            <Style TargetType="Expander">
                                <Setter Property="Foreground" Value="{StaticResource NavAreaFontColor}"/>
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate x:Name="control"  TargetType="Expander">
                                            <ControlTemplate.Resources>
                                                <ResourceDictionary>
                                                    <FontFamily x:Key="FontAwesome">/AE.Graphics.Wpf;Component/Fonts/fontawesome-webfont.ttf#FontAwesome</FontFamily>
                                                </ResourceDictionary>
                                            </ControlTemplate.Resources>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Name="ContentRow" Height="0"/>
                                                </Grid.RowDefinitions>
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="0" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border BorderBrush="DarkGray" BorderThickness="0,0,0,1" Margin="5,0,5,8" >
                                                        <TextBlock x:Name="HeaderText" Text="{Binding Path=Name}"/>
                                                    </Border>
                                                    
                                                </Grid>
                                                <Border Name="Content" Grid.Row="1">
                                                    <ContentPresenter />
                                                </Border>
                                            </Grid>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsExpanded" Value="True">
                                                    <Setter TargetName="ContentRow" Property="Height" Value="{Binding ElementName=Content,Path=DesiredHeight}" />
                                                    <!--<Setter TargetName="Toggle" Property="Content" Value="&#xf13a;"/>-->
                                                </Trigger>
                                                
                                                <Trigger Property="IsExpanded" Value="False">
                                                    <!--<Setter TargetName="Toggle" Property="Content" Value="&#xf137;"/>-->
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Expander.Style>
                        
                        <ItemsPresenter />
                    </Expander>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="AEThumbStyle" TargetType="Thumb">
        <Setter Property="SnapsToDevicePixels" Value="true"/>
        <Setter Property="OverridesDefaultStyle" Value="true"/>
        <Setter Property="IsTabStop" Value="false"/>
        <Setter Property="Focusable" Value="false"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <Border Background="{StaticResource WindowBorderColor}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!-- AE Window end-->

    <!-- Flat Button -->
    <Style x:Key="AEFlatButtonStyle" TargetType="Button">
        <Setter Property="Foreground">
            <Setter.Value>
                <SolidColorBrush Opacity="1" Color="White"/>
            </Setter.Value>
        </Setter>

        <Setter Property="Background" >
            <Setter.Value>
                <SolidColorBrush Opacity="1" Color="#39393d" />
            </Setter.Value>
        </Setter>

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">

                    <Border x:Name="border" SnapsToDevicePixels="True" BorderBrush="{TemplateBinding Background}" BorderThickness="1" Padding="2" Background="{TemplateBinding Background}">
                        <Grid>
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" RecognizesAccessKey="True" />
                        </Grid>
                    </Border>

                </ControlTemplate>
            </Setter.Value>
        </Setter>

        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
